@model InvestmentManagement.InvestmentManagement.Models.BROKER

<div id="Replace">
    <div class="contentTop">
        <span class="pageTitle">
            <span class=" glyphicon glyphicon-yen"></span>
            @Session["currentPage"]
        </span>

    </div>
    <ol class="breadcrumb">
        @Html.Raw(@ViewBag.BreadCum)
    </ol>
    <div class="wrapper pl">
        <div class="alert alert-success mt20">
            <button class="close" data-dismiss="alert" type="button">×</button>
            @ViewBag.Message
        </div>
        <div class="container-fluid">

            <div class="widget">
                <div class="whead">
                    <h6>
                        <label id="lblHeader" title="">@ViewBag.Header</label>
                    </h6>
                    <div class="clear">
                    </div>
                </div>
                @using (Ajax.BeginForm("AddBroker", "Broker", new AjaxOptions { HttpMethod = "POST", InsertionMode = System.Web.Mvc.Ajax.InsertionMode.Replace, UpdateTargetId = "Replace", OnBegin = "ShowProcessingMsg", OnSuccess = "HideProcessingMsg" }, new { autocomplete = "off" }))
                { 

                    <div class="entryForm">


                        <ul class="col-md-6 entryForm ">

                            <li class="form-group">


                                <label class="control-label entrylable">TREC Code</label>

                                @Html.TextBoxFor(model => model.MEMBERID, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.MEMBERID) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.MEMBERID) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.MEMBERID)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.BONUMBER, new { @class = "control-label entrylable" })


                                @Html.TextBoxFor(model => model.BONUMBER, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.BONUMBER) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.BONUMBER) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.BONUMBER)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.CSEEXCHANGEID, new { @class = "control-label entrylable" })

                                @Html.TextBoxFor(model => model.CSEEXCHANGEID, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.CSEEXCHANGEID) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.CSEEXCHANGEID) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.CSEEXCHANGEID)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.CSECLEARINGBO, new { @class = "control-label entrylable" })

                                @Html.TextBoxFor(model => model.CSECLEARINGBO, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.CSECLEARINGBO) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.CSECLEARINGBO) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.CSECLEARINGBO)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.COMMISSIONRATE, new { @class = "control-label entrylable" })

                                <div class="input-group">
                                    @Html.TextBoxFor(model => model.COMMISSIONRATE, new { @class = "form-control decimal", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.COMMISSIONRATE) + " (Only Numeric)", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.COMMISSIONRATE) + "'" })
                                    <div class="input-group-addon">.00</div>
                                </div>
                                <br />
                                @Html.ValidationMessageFor(model => model.COMMISSIONRATE)

                            </li>
                        </ul>

                        <ul class="col-md-6 entryForm">

                            <li class="form-group">

                                @*@Html.LabelFor(model => model.CDBLID,new {@class="control-label entrylable"})*@

                                <label class="control-label entrylable">CDBL DP ID</label>

                                @Html.TextBoxFor(model => model.CDBLID, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.CDBLID) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.CDBLID) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.CDBLID)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.DSEEXCHANGEID, new { @class = "control-label entrylable" })

                                @Html.TextBoxFor(model => model.DSEEXCHANGEID, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.DSEEXCHANGEID) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.DSEEXCHANGEID) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.DSEEXCHANGEID)

                            </li>
                            <li class="form-group">
                                <label class="control-label entrylable">CDBL Clearing BO</label>
                                @* @Html.LabelFor(model => model.DSECLEARINGBO,new {@class="control-label entrylable"})*@
                                @Html.TextBoxFor(model => model.DSECLEARINGBO, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.DSECLEARINGBO) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.DSECLEARINGBO) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.DSECLEARINGBO)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.NAME, new { @class = "control-label entrylable" })
                                @Html.TextBoxFor(model => model.NAME, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.NAME) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.NAME) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.NAME)

                            </li>
                            <li class="form-group">

                                @Html.LabelFor(model => model.DEFAULTTRADER, new { @class = "control-label entrylable" })

                                @Html.TextBoxFor(model => model.DEFAULTTRADER, new { @class = "form-control", @placeholder = "Please Enter Your " + @Html.DisplayNameFor(model => model.DEFAULTTRADER) + "", @onfocus = "this.placeholder = ''", @onblur = "this.placeholder = '" + @Html.DisplayNameFor(model => model.DEFAULTTRADER) + "'" })

                                <br />
                                @Html.ValidationMessageFor(model => model.DEFAULTTRADER)

                            </li>
                        </ul>


                        <button type="submit" class="btn btn-success lastli" tabindex="">Save</button>
                        <a class="load btn btn-danger closeButton" href="~/#/Broker/ListBroker?lblbreadcum=">Close</a>

                    </div>



                }

            </div>


        </div>
    </div>
    <p></p>


    <script type="text/javascript">
        function ShowProcessingMsg() {

            $('.loadingImage').show();

        }
        function HideProcessingMsg() {
            $('.loadingImage').hide();
        }

    </script>
    <script>

        $(function () {
            $('.datepicker').datepicker({ dateFormat: "dd/mm/yy" });

            //$(".dropdown-menu li a").click(function (event) {
            //var selText = $(this).text();
            //$('.hidden').val(selText);
            //$(this).parents('.btn-group').find('.dropdown-toggle').html(selText + ' <span class="caret"></span>');
            //event.preventDefault();
            //});

        });

    </script>

    <script>
        $(document).on('keypress', '.number', function (event) {

            if (event.keyCode == 46 || event.keyCode == 8) {
                // let it happen, don't do anything
            } else if ((event.which < 48 || event.which > 57)) {
                event.preventDefault();
            }

        });
        $(document).on('keypress', '.decimal', function (event) {

            if (event.keyCode == 46 || event.keyCode == 8) {
                // let it happen, don't do anything
            } else if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 48 || event.which > 57)) {
                event.preventDefault();
            }

        });


    </script>

</div>

